{
  "ingress": {
    "type": "kafka",
    "settings": {
      "bootstrapServers": "localhost:9092",
      "topic": "site-topic",
      "groupId": "nstream-jet",
      "autoOffset": "latest",
      "keyDeserializer": "integer",
      "valueDeserializer": "avro",
      "pollTimeoutMillis": 100
    },
    "relay": {
      "agent": "site",
      "idExtractor": "$key",
      "valueExtractor": "$value"
    }
  }
}
