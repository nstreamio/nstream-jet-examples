{
  "ingress": {
    "type": "kafka",
    "settings": {
      "bootstrapServers": "localhost:9092",
      "topic": "site-topic",
      "groupId": "nstream-jet",
      "keyDeserializer": "integer",
      "valueDeserializer": "string",
      "valueContentType": "json"
    },
    "relay": {
      "agent": "site",
      "idExtractor": "$key",
      "valueExtractor": "$value.status"
    }
  },
  "agent": {
    "name": "site",
    "patches": ["latest"]
  }
}
