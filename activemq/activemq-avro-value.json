{
  "ingress": {
    "type": "activemq",
    "settings": {
      "brokerURL": "tcp://host.docker.internal:61616",
      "destination": "topic://my-topic",
      "contentType": "avro"
    },
    "relay": {
      "agent": "message",
      "idExtractor": "$id",
      "valueExtractor": "$value"
    }
  }
}
